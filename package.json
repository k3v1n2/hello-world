  "scripts": {
    "prebuild": "npm-run-all clean-dist flow test lint stylelint copy:constants",
    "build": "babel-node helpers/build.js",
    "postbuild": "babel-node helpers/distServer.js",
    "prebuild:only": "npm run flow",
    "build:only": "babel-node --max_old_space_size=4096 helpers/build.js",
    "postbuild:only": "npm run copy:constants",
    "prebuild:styleguide": "npm-run-all clean-dist:styleguide",
    "build:styleguide": "babel-node helpers/buildStyleguide.js",
    "postbuild:styleguide": "babel-node helpers/styleguideServer.js",
    "build:styleguide:only": "babel-node --max_old_space_size=4096 helpers/buildStyleguide.js",
    "clean-dist": "rimraf ./app/dist && mkdir app/dist",
    "clean-dist:styleguide": "rimraf ./styleguide/dist && mkdir styleguide/dist",
    "copy:constants": "cpx ./app/src/constants.js ./app/dist/",
    "flow": "flow check",
    "generate:component": "babel-node helpers/componentGenerator.js",
    "generate:mock-data": "babel-node helpers/generateMockData",
    "lib:sync": "cd docs && fractal start --sync",
    "lib:build": "cd docs && fractal build",
    "lint": "esw --ext .js --ext .jsx webpack.confg.* helpers app/src components styleguide/src --color",
    "lint:watch": "npm run lint -- --watch",
    "security-check": "nsp check",
    "check-all": "npm-run-all --parallel flow lint test stylelint",
    "start-all": "npm-run-all --parallel lint:watch test:watch stylelint:watch start:mockapi start:dev",
    "start": "npm run start:dev",
    "start:dev": "webpack-dev-server --config webpack.config.dev.babel.js --open --https",
    "start:mockapi": "json-server --watch app/src/api/db.json --port 3009",
    "styleguide": "npm-run-all --parallel lint:watch test:watch stylelint:watch styleguide:dev",
    "styleguide:dev": "webpack-dev-server --config webpack.config.styleguide.dev.babel.js --open",
    "stylelint": "stylelint \"app/src/**/!(_component).scss\" --syntax scss",
    "stylelint:watch": "onchange \"app/src/**/!(_component).scss\"  -i -w -d 5000 -- npm run stylelint",
    "test": "mocha --max-old-space-size=4096 --require babel-polyfill helpers/browser.js --recursive \"./{,!(node_modules)/**/}!(_component).spec.js*\"",
    "test:coverage": "nyc mocha --max_old_space_size=4096 --require babel-polyfill helpers/browser.js --recursive \"./{,!(node_modules)/**/}!(_component).spec.js*\"",
    "test:watch": "npm run test -- --watch",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "nyc": {
    "exclude": [
      "**/*.spec.js",
      "dist",
      "docs",
      "helpers",
      "node_modules",
      "styleguide",
      "webpack*"
    ],
    "extensions": [
      ".js",
      ".jsx"
    ]
  }
}
